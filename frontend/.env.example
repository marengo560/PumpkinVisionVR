# Pumpkin Control - Environment Configuration
# Copy this file to .env and update with your values

# Backend API URL - UPDATE THIS BEFORE BUILDING APK!
# For local WiFi network (no internet needed):
EXPO_PUBLIC_BACKEND_URL=http://192.168.1.100:8001

# Examples:
# Local Jetson: http://192.168.1.100:8001
# Internet server: https://your-domain.com

# DO NOT MODIFY THESE (used by Expo development server):
EXPO_TUNNEL_SUBDOMAIN=headgear-ssh
EXPO_PACKAGER_HOSTNAME=https://headgear-ssh.preview.emergentagent.com
EXPO_USE_FAST_RESOLVER="1"
METRO_CACHE_ROOT=/app/frontend/.metro-cache
